<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d-[%thread] %-5level - %msg%n</pattern>
		</encoder>
	</appender>

	<root level="ERROR">
		<appender-ref ref="STDOUT" />
	</root>

	<logger name="com.hm.sample" level="ERROR">
	</logger>

</configuration>
